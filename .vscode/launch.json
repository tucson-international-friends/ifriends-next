	"version": "0.2.0",
	"configurations": [
		
		{
			"type": "node",
			"request": "launch",
			"name": "Next: Node",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "dev"],
			"port": 9229,
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Next: Chrome",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}"
		}
	],
	"compounds": [
		{
			"name": "Next: Full",
			"configurations": ["Next: Node", "Next: Chrome"]
		}
	]
}
